.label-left{vertical-align:top; padding-top:8px;font-weight:bold;text-align:right;margin-right:10px;display:inline-block}
.label-static{padding-top:8px;padding-bottom:5px;display:inline-block}
.control{display:inline-block}
.control span{padding-top:8px;display:inline-block}
.clear{clear:left}
.error{color:Red;font-weight:bold;text-shadow: 1px 1px 2px white}
.pointer{cursor:pointer}
#attachmentFileList{list-style-type: none;padding: 0px;margin: 0px;}
#attachmentFileList input{display:none}
#attachmentFileList li{background-color: #f5f5f5;
    border: 1px solid rgb(209, 214, 144);
    margin: 2px 0px;
    overflow-y: hidden;
    font-weight:bold;
    padding: 2px 2px 2px 3px;}
#attachmentFileList a{color:#15c;margin-left:5px}
.attach-del-btn{background:url("../Images/x_8px.png") no-repeat 0 0;cursor: pointer;height: 16px;width: 16px;float:right;margin:-2px 2px 1px 2px}
.assign-emp{border-radius:4px;padding:2px 3px;margin-bottom:5px;background:green;color:yellow}
.assign-team{border-radius:4px;padding:2px 3px;margin-bottom:5px;background:#293955;color:white}
.assignbranch{border-radius:4px;padding:2px 3px;margin-bottom:5px;background:#E59C0F;color:white}


